<div class="row text-center">
  <h1>Where ya wanna go?</h1><hr/><br>
   <%= form_tag search_path, :method => 'get' do %>
    <div class="large-8 small-9 columns nav-padding">
      <%= text_field_tag :city, params[:city], placeholder: "City" %>
      <%= text_field_tag :state, params[:state], placeholder: "State" %>
    </div><br>
    <div class="large-4 small-3 columns nav-padding">
      <button class="alert button expand" id="click_search" name="search_button" type="submit" value="Search">Search
      </button>
    </div>
  <% end %>
  <hr/>

  <%@jobs.each do |job|%>
    <div class="panel">
      <%= job[:title] %>
      <%= link_to "link", "#{job[:link]}" %>
    </div>
  <%end%>
</div>